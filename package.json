{
    "name": "pix-companion",
    "description": "Un Companion pour vous accompagner dans la certification de vos compenténces numérique",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "wxt",
        "dev:firefox": "wxt -b firefox",
        "dev:edge": "wxt -b edge",
        "build": "wxt build",
        "build:firefox": "wxt build -b firefox",
        "build:edge": "wxt build -b edge",
        "zip": "wxt zip",
        "zip:firefox": "wxt zip -b firefox",
        "zip:edge": "wxt zip -b edge",
        "zip:safari": "wxt zip -b safari",
        "compile": "vue-tsc --noEmit",
        "postinstall": "wxt prepare"
    },
    "dependencies": {
        "asciidoctor": "^2.2.7",
        "vue": "^3.4.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.1",
        "typescript": "^5.3.3",
        "vue-tsc": "^1.8.27",
        "wxt": "^0.17.0"
    }
}
